{
  "workbench": {
    "colorTheme": "GitHub Dark Dimmed"
  },

  "window": {
    "commandCenter": false
  },

  "files": {
    "insertFinalNewline": true,
    "trimFinalNewlines": true,
    "trimTrailingWhitespace": true
  },

  "editor": {
    "tabSize": 2,
    "wordWrapColumn": 120
  },

  "terminal": {
    "integrated": {
      "defaultProfile": {
        "osx": "fish"
      }
    }
  }
}
